package com;

public class Car {

	public static void main(String[] args) {
		int wheel;
		String color;
		float price;			// instance variable 
	}
		void start() {
			System.out.println("Car Started...");
		}
		void appliedGear() {
			System.out.println("Applied Gear");
		}
		void moving() {
			System.out.println("Car is moving");
		}
		void stop() {
			System.out.println("Car Stop");
		}
		

		
		}


